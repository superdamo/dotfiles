!colour scheme

*foreground:	#8fa388
*background:	#151b1a

!black & dark grey
*color0:	#2e2b38
*color8:	#474159
!dark red & red
*color1:	#984e66
*color9:	#a86d56
!dark green & green
*color2:	#7a8c54
*color10:	#9fb274
!dark yellow & yellow
*color3:	#9e6438
*color11:	#b29f5e
!dark blue & blue
*color4:	#395667
*color12:	#507a91
!dark magenta & magenta
*color5:	#9d916b
*color13:	#b89855
!dark cyan & cyan
*color6:	#536556
*color14:	#7e9982
!light grey & white
*color7:	#828181
*color15:	#bdbda4

*corsorColor:	#9e6438


!cursor theme

Xcursor.theme: /usr/share/icons/DMZ-Black/cursor.theme


!fix the XTerm alt key

XTerm*metaSendsEscape: true


!setting the font

XTerm*faceName: Pixel Operator
XTerm*faceSize: 11

