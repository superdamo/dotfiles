Config	{

	font="xft:Pixel Operator:size=11"
	,bgColor="#1a1a12"
	,fgColor="#bfb4a7"
	,border=FullB
	,borderColor="#364544"
	,borderWidth=4
	,position=Static { xpos=16, ypos=16, width=1334, height=29 }
	,lowerOnStart=False
	,hideOnStart=False
	,persistent=True
	,sepChar="%"
	,alignSep="}{"
	,template="  %StdinReader% } %cmus% { %wlan0wi% | %battery% | %bright% |%volume% | %date%  "
	,commands=
		[Run Wireless "wlan0" ["-t", "<essid>: <quality>%"] 10
		,Run BatteryP ["BAT0"] ["-t", "Battery: <left>%", "--", "-O", "charging, ", "-o", ""] 30
		,Run Com "bash" ["/home/damien/.brightness"] "bright" 1
		,Run Com "bash" ["/home/damien/.getvolume.sh"] "vol" 1
		,Run Date "%a %d %b %H:%M" "date" 100
		,Run Com "bash" ["/home/damien/.cmus_info.sh"] "cmus" 1
		,Run StdinReader
		]
}
