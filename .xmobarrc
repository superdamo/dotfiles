Config	{

	font="Pixel Operator:size=11"
	,bgColor="#151b1a"
	,fgColor="#8fa388"
	,border=FullB
	,borderColor="#bdbda4"
	,borderWidth=6
	,position=Static { xpos=16, ypos=16, width=1334, height=29 }
	,lowerOnStart=False
	,hideOnStart=False
	,persistent=True
	,sepChar="%"
	,alignSep="}{"
	,template=" StdinReader } %cmus% { %wifi% | %battery% | %vol% | %date% "
	,commands=
		[Run Com "bash" ["/home/damien/.wireless.sh"] "wifi" 1
		,Run BatteryP ["BAT0"] ["-t", "Battery: <left>%", "--", "-O", "charging, ", "-o", ""] 30
		,Run Com "bash" ["/home/damien/.getvolume.sh"] "vol" 10
		,Run Date "%a %d %b %k:%M" "date" 100
		,Run Com "bash" ["/home/damien/.cmus_info.sh"] "cmus" 1
		,Run StdinReader
		]
}
